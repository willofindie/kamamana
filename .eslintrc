{
  "parser": "babel-eslint",
  "extends": ["plugin:react/recommended", "plugin:prettier/recommended"],
  "plugins": ["react", "jsx-a11y", "prettier"],
  "rules": {
    "prettier/prettier": "error"
  },
  "env": {
    "browser": true
  },
  "settings": {
    "react": {
      "version": "16.7.0",
      "flowVersion": "0.89.0"
    }
  },
  "overrides": [
    {
      "files": ["src/**/*.js"]
    }
  ]
}
