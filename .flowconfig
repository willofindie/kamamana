[ignore]
.*/dist/.*
.*/node_modules/.*
.*/.storybook/.*
.*/stories/.*
.*/__tests__/.*
.*/temp/.*
.*/rollup.config.js

[include]
.*/src/.*

[libs]
flow-typed/npm/nano-css_v3.4.0.js
flow-typed/npm/jest_v23.x.x.js

[lints]

