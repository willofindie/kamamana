[ignore]
.*/dist/.*
.*/node_modules/.*
.*/.storybook/.*
.*/.jest/.*
.*/.out/.*
.*/__tests__/.*
.*/temp/.*
.*/rollup.config.js
.*/examples/.*
.*/src/__image_snapshots__/.*
.*/stories/.*/__snapshots__/.*

[include]
.*/src/*.js

[libs]
flow-typed/npm/nano-css_vx.x.x.js
flow-typed/npm/jest_v23.x.x.js
flow-typed/npm/@storybook/react_v4.x.x.js
flow-typed/npm/@storybook/addon-storyshots_vx.x.x.js
flow-typed/npm/@storybook/addon-storyshots-puppeteer_vx.x.x.js
flow-typed/npm/@fortawesome/fontawesome-svg-core_vx.x.x.js
flow-typed/npm/@fortawesome/react-fontawesome_vx.x.x.js
flow-typed/npm/@fortawesome/free-solid-svg-icons_vx.x.x.js
flow-typed/npm/@material-ui/icons_vx.x.x.js
flow-typed/npm/@material-ui/core_vx.x.x.js

[lints]

[options]
all=true
module.name_mapper='^/src' ->'<PROJECT_ROOT>/src'
module.name_mapper='^/components' ->'<PROJECT_ROOT>/src/components'
module.name_mapper='^/utils' ->'<PROJECT_ROOT>/src/utils'
module.name_mapper='^kamamana' ->'<PROJECT_ROOT>/dist/index.min'
# https://github.com/facebook/flow/issues/3850
module.ignore_non_literal_requires=true

[strict]
